<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
    <link href="vbxapp.css" rel="stylesheet">
    <script src="vbxapp.js"></script>
    <script>
        VBX.App.init({
            feed: {
                container: 'feed-container1', template: 'item-template1', source: 'feedXmlString'
            }
        });
    </script>
    <script> function getAppConfigs() {
        return [[[= JSON.stringify(model.getAppConfigs)]]];
    }
    </script></head>
<body class="vbx-app-rssitem">
<div class="vbx-scrollable">
    <div id="feed-container1" class="vbx-feed-container">
        <div id="item-template1" class="vbx-feed-item">
            {if $thumbnail}<img class="vbx-feed-thumbnail" src="{$thumbnail}" data-vbx-style-width="VBX.px(thumbnailWidth)" />{/if}
            {if $author}<div class="vbx-feed-author">{$author}</div>{/if}
            <div class="vbx-feed-title">{$title}</div>
            <div class="vbx-feed-content">{$content}</div>
            {if $publishedDate}<div class="vbx-feed-date">{$publishedDate}</div>{/if}
        </div>
    </div>
</div>
</body>
</html>

