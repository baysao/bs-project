    <style>
        body {text-align:center; overflow:hidden}

        @media screen {
            .coupon {
                position: absolute;
                left: 0;
                top: 3em;
                right: 0;
                bottom: 3em;
                background-repeat: no-repeat;
                background-position: center center;
                background-size: contain;
            }

            .overlay-top {
                position: absolute;
                background-color: rgba(0,0,0,0.8);
                left: 0;
                top: 0;
                right: 0;
                height: 3em;
                line-height: 3em;
            }

            .overlay-bottom {
                position: absolute;
                background-color: rgba(0,0,0,0.8);
                left: 0;
                bottom: 0;
                right: 0;
                height: 3em;
                line-height: 3em;
            }

            .vbx-light-style .overlay-top, .vbx-light-style .overlay-bottom {
                background-color: rgba(255,255,255,0.8);
                color: #000000;
            }

            .overlay-bottom button {
                line-height: 0em;
                margin-top: 5px;
            }

            .print-only {
                display: none;
            }
        }

        @media print {
            .overlay-top, .overlay-bottom, .coupon {
                display: none;
            }

            .print-only {
                position:absolute;
                left:0;top:0;
                width:100%;
                overflow:hidden;
                display: block;
                text-align:center;
                background-color: white;
            }

            .print-only img {
                max-width:80%;
                max-height:80%;
            }
        }
    </style>
