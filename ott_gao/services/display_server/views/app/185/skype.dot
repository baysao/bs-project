<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.0/jquery.min.js"></script>
    <link href="vbxapp.css" rel="stylesheet">
    <script src="vbxapp.min.js"></script>
    <script>
        var address = '';

        VBX.App.init({
            ready: function () {
                VBX.App.listen('address', function (value) {
                    address = value;
                    updateButton();
                });
            }
        });

        function updateButton() {
            var buttonText;
            var numericRegExp = /^[0-9+ .()-]*$/;
            if (numericRegExp.test(address)) {
                buttonText = 'Call with Skype';
            } else {
                buttonText = 'Chat on Skype';
            }
            document.querySelector('#call-button').textContent = buttonText;
        }

        function clickButton() {
            var numericRegExp = /^[0-9+ .()-]*$/;
            if (numericRegExp.test(address)) {
                window.open('skype:+' + address + '?call', 'skypeout', 'width=472,height=320,resizable=yes');
            } else {

                window.open('skype:' + address + '?chat', 'skypechat', 'width=472,height=320,resizable=yes');
            }
        }
    </script>

    <style>
        #skype-logo {margin-bottom: 1em;}
    </style>
