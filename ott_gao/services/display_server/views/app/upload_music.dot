    <link href="vbxapp.css" rel="stylesheet">
    <script src="vbxapp.min.js"></script>
    <script>
        VBX.App.init({
            feed: {
                container: 'feed-container1', template: 'item-template1', source: 'feedXmlString', artist: function (entry, xml, index) {
                    return xml.querySelector('artist').textContent;
                },
                audioUrl: function (entry, xml, index) {
                    return xml.querySelector('audioUrl').textContent;
                },
                onBuilt: function () {
                    $('audio').on('play', function (e) {
                        $('audio').each(function (index) {
                            if (this != e.target) {
                                this.pause();
                            }
                        });
                    });
                }
            },
            pauseAllMedia: function () {
                $('audio').each(function (index) {
                    this.pause();
                });
            }
        });

    </script>

    <style>
        .image {
            float: left;
            margin-right: 5px;
            margin-bottom: 5px;
        }

        .header {
            margin-bottom:1em;
        }
