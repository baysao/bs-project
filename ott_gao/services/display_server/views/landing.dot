      xmlns:og="http://opengraphprotocol.org/schema/" xmlns:g="http://base.google.com/ns/1.0">
<head id="Head1">
    [[[= partial('partials/seo.html')]]]
    <script type="text/javascript">
        var queryVars = {};
    </script>
    <style type="text/css">
        body
        {
            padding: 0;
            margin: 0;
            width: 100%;
            display: table;
            background: #000000; /* Old browsers */
            background: -moz-linear-gradient(left,  #000000 0%, #333333 40%, #333333 60%, #000000 100%); /* FF3.6+ */
            background: -webkit-gradient(linear, left top, right top, color-stop(0%,#000000), color-stop(40%,#333333), color-stop(60%,#333333), color-stop(100%,#000000)); /* Chrome,Safari4+ */
            background: -webkit-linear-gradient(left,  #000000 0%,#333333 40%,#333333 60%,#000000 100%); /* Chrome10+,Safari5.1+ */
            background: -o-linear-gradient(left,  #000000 0%,#333333 40%,#333333 60%,#000000 100%); /* Opera 11.10+ */
            background: -ms-linear-gradient(left,  #000000 0%,#333333 40%,#333333 60%,#000000 100%); /* IE10+ */
            background: linear-gradient(to right,  #000000 0%,#333333 40%,#333333 60%,#000000 100%); /* W3C */
            filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#000000', endColorstr='#000000',GradientType=1 ); /* IE6-9 */
        }
        #viewbix_center_container
        {
            width: 100%;
            margin-left: auto;
            margin-right: auto;
            padding: 20px 0;
            text-align: center;
            position: relative;
            top: 70px;
            background: #111111; /* Old browsers */
            background: -moz-linear-gradient(left, #111111 0%, #444444 50%, #111111 100%); /* FF3.6+ */
            background: -webkit-gradient(linear, left top, right top, color-stop(0%,#111111), color-stop(50%,#444444), color-stop(100%,#111111)); /* Chrome,Safari4+ */
            background: -webkit-linear-gradient(left, #111111 0%,#444444 50%,#111111 100%); /* Chrome10+,Safari5.1+ */
            background: -o-linear-gradient(left, #111111 0%,#444444 50%,#111111 100%); /* Opera 11.10+ */
            background: -ms-linear-gradient(left, #111111 0%,#444444 50%,#111111 100%); /* IE10+ */
            background: linear-gradient(to right, #111111 0%,#444444 50%,#111111 100%); /* W3C */
            filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#111111', endColorstr='#111111',GradientType=1 ); /* IE6-9 */
        }

        #player_container
        {
            width: 747px;
            margin: 0 auto;
        }

        #responsive-outer {
            position:relative;
            padding:0px 0px 0px 0px;
        }

        #responsive-inner {
            width:100%;
            height:0;
            padding-bottom:56.25%;
            padding-top:8px;
        }

        #player {
            position:absolute;
            top:0;
            left:0;
            width:100%;
            height:100%;
            border:none;
        }

        @media only screen and (max-width : 767px) {
            body {
                background: #000000;
            }

            #viewbix_center_container
            {
                position:absolute;
                height:100%;
                padding: 0;
                top: 0;
                background: #000000;
            }

            #player_container
            {
                width: 100%;
                height:100%;
            }

            #responsive-outer {
                width:100%;
                height:100%;
                padding:0;
            }

            #responsive-inner {
                height:100%;
                padding-top:0px;
                padding-bottom:0px;
            }
        }
    </style>
</head>
<body class="jolt">
<div id="viewbix_center_container">
    <div id="player_container">
        <div id="responsive-outer"><div id="responsive-inner"><iframe id="player" width="100%" height="100%" src="/frame/[[[= model.sid]]]?&w=747&h=428&ap=true" frameborder="0" scrolling="no" allowTransparency="true" allowfullscreen></iframe></div></div>
    </div>
</div>
[[[= partial('partials/vbx_tracking.html')]]]
</body>
</html>
